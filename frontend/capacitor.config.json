{
  "appId": "tech.edusystem",
  "appName": "EduSystem",
  "webDir": "dist",
  "assets": "./assets",
 "plugins": {
    "SplashScreen": {
      "launchShowDuration": 6000,
      
      "launchFadeOutDuration": 8000,
      "backgroundColor": "#e21eee",
      "androidSplashResourceName": "splash",
  
      "showSpinner": true,
      "androidSpinnerStyle": "large",
      "iosSpinnerStyle": "big",
      "spinnerColor": "#e21eee",
      "splashFullScreen": true,

      "layoutName": "launch_screen",
      "useDialog": true
    }
  }
}